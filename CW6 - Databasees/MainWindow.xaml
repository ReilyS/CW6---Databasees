<Window x:Class="del.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:del"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FF383434">
    <Grid>
        <TextBlock x:Name="txt" HorizontalAlignment="Left" Margin="48,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="269" Width="369" Background="#FFD4D4D4" FontSize="24"/>
        <Button x:Name="assets_button" Content="See Assets" HorizontalAlignment="Left" Margin="509,62,0,0" VerticalAlignment="Top" Width="161" Click="See_Assets" Height="88" TextOptions.TextHintingMode="Fixed"/>
        <Button x:Name="employees_button" Content="See Employees" HorizontalAlignment="Left" Margin="509,189,0,0" VerticalAlignment="Top" Width="161" Click="See_Employes" Height="88"/>

    </Grid>
</Window>
